<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.QrCodeTry1" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/appbrightblue</item>
        <item name="colorPrimaryVariant">@color/appblue</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/contrast</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->


        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.QrCodeTry1.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="android:statusBarColor">@color/appbrightblue</item>
        <item name="android:windowActionBarOverlay">true</item>


    </style>


    <style name="AlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/appbrightblue</item>
        <item name="android:textColorPrimary">#FFFFFF</item>
        <item name="android:background">@color/appblue</item>
    </style>

    <style name="Theme.QrCodeTry1.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>